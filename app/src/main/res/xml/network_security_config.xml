<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">www3.moneris.com</domain>
        <domain includeSubdomains="true">esqa.moneris.com</domain>
        <domain includeSubdomains="true">moneris.com</domain>
        <trust-anchors>
            <certificates src="@raw/entrust_2048_ca" />
            <certificates src="@raw/entrust_ec1_ca" />
            <certificates src="@raw/entrust_ev_ca" />
            <certificates src="@raw/entrust_g2_ca" />
            <certificates src="@raw/entrust_g3_ca" />
